#: src/org.cafe.terminal.gschema.xml.in:295
msgid "Highlight URLs under mouse pointer"
msgstr "Evidenzia URL al passaggio del mouse"

#: src/org.cafe.terminal.gschema.xml.in:296
msgid ""
